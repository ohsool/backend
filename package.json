{
  "name": "ohsool",
  "version": "1.0.0",
  "description": "ohsool; recommending drink of a day",
  "main": "app.js",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "scripts": {
    "start": "node dist/app.js",
    "dev": "tsc && nodemon src/app.ts",
    "prettify": "prettier --write src/*.ts src/**/*.ts",
    "build": "tsc -p .",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "joi": "^17.4.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.13.3",
    "supertest": "^6.1.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^16.4.1",
    "@types/supertest": "^2.0.11",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ohsool/backend.git"
  },
  "bugs": {
    "url": "https://github.com/ohsool/backend/issues"
  },
  "homepage": "https://github.com/ohsool/backend#readme"
}
